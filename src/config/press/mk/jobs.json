{
  "_id": "mk",
  "daemon": {
    "sleep": 1800
  },
  "trace_list": [
    {
      "host": "https://corpus.ncsoft.com:9200",
      "http_auth": "crawler:crawler2019",
      "index": "crawler-news-mk",
      "split_index": true,
      "sleep": 2,
      "step": 1,
      "end": 5001,
      "start": 1,
      "list": [
        {
          "url_frame": "https://www.mk.co.kr/news/society/general/?page={page}",
          "category": "사회/일반"
        }
      ],
      "article": {
        "document_id": {
          "type": "path",
          "replace": [
            {
              "from": "/news",
              "to": ""
            },
            {
              "from": "/sports",
              "to": ""
            },
            {
              "from": "/view",
              "to": ""
            },
            {
              "from": "/society",
              "to": ""
            },
            {
              "from": "^/",
              "to": ""
            },
            {
              "from": "/$",
              "to": ""
            },
            {
              "from": "/",
              "to": "-"
            }
          ]
        }
      },
      "post_process": []
    }
  ]
}
