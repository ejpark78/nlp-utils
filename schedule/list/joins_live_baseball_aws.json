{
    "_id": "joins_live_baseball",
    "docker": {
        "command": "python3 scheduler.py",
        "image": "crawler:1.0",
        "working_dir": "/usr/local/app"
    },
    "parameter": {
        "db_info": {
            "corpus-process": [
                {
                    "host": "http://elastic01:5004/v1.0/api/batch"
                },
                {
                    "host": "http://elastic02:5004/v1.0/api/batch"
                }
            ],
            "db_name": "joins_live_baseball"
        },
        "delay": "15~20",
        "document_id": "{_id}",
        "end_date": "2018-12-31",
        "max_skip": 5000,
        "parsing_info": "joins-live.sports",
        "start_date": "2018-01-01",
        "url_frame": [
            {
                "const_value": {
                    "section": "스포츠-야구"
                },
                "url": "http://isplus.live.joins.com/news/list/list.asp?dt={date}&tm=i_b_6110"
            }
        ]
    },
    "schedule": {
        "group": "aws",
        "mode": "daemon",
        "sleep": "20",
        "sleep_range": "03,04"
    },
    "state": {
        "host": "gollum01",
        "progress": "0.5",
        "running": "2018-01-03",
        "state": "running"
    }
}
