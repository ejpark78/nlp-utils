{
    "_id": "donga_sports",
    "docker": {
        "command": "python3 scheduler.py",
        "image": "crawler:1.0",
        "working_dir": "/usr/local/app"
    },
    "parameter": {
        "const_value": {
            "section": "야구"
        },
        "db_info": {
            "corpus-process": [
                {
                    "host": "http://elastic01:5004/v1.0/api/batch"
                },
                {
                    "host": "http://elastic02:5004/v1.0/api/batch"
                }
            ],
            "db_name": "donga_baseball"
        },
        "delay": "10~15",
        "document_id": "{_id}",
        "end": "3",
        "max_skip": 5000,
        "parsing_info": "donga.sports",
        "start": "1",
        "url_frame": [
            {
                "const_value": {
                    "section": "야구"
                },
                "url": "http://news.donga.com/List/Sports/Base?p={start}&prod=news&ymd=&m=NP"
            }
        ]
    },
    "schedule": {
        "group": "aws",
        "mode": "daemon",
        "sleep": "20",
        "sleep_range": "03,04"
    },
    "state": {}
}
