{
    "_id": "spotv_baseball",
    "docker": {
        "command": "python3 scheduler.py",
        "image": "crawler:1.0",
        "working_dir": "/usr/local/app"
    },
    "parameter": {
        "const_value": {
            "section": "스포츠-야구"
        },
        "db_info": {
            "corpus-process": [
                {
                    "host": "http://elastic01:5004/v1.0/api/batch"
                },
                {
                    "host": "http://elastic02:5004/v1.0/api/batch"
                }
            ],
            "db_name": "spotv_baseball",
            "s3": {
                "bucket": "paige-cdn-origin",
                "url_prefix": "http://paige-cdn.plaync.com"
            }
        },
        "delay": "15~20",
        "document_id": "{_id}",
        "end": "2",
        "max_skip": 5000,
        "parsing_info": "spotv.sports",
        "start": "1",
        "url_frame": [
            {
                "const_value": {
                    "section": "스포츠-야구"
                },
                "url": "http://www.spotvnews.co.kr/?page={start}&mod=news&act=articleList&total=38674&sc_code=1384128643&view_type=S"
            }
        ]
    },
    "schedule": {
        "group": "aws",
        "mode": "daemon",
        "sleep": "20",
        "sleep_range": "03,04"
    },
    "state": {
        "host": "gollum01",
        "start": "731",
        "state": "running"
    }
}
