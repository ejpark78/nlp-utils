{
    "_id": "khan_baseball",
    "docker": {
        "command": "python3 scheduler.py",
        "image": "crawler:1.0",
        "working_dir": "/usr/local/app"
    },
    "parameter": {
        "const_value": {
            "section": "스포츠-야구"
        },
        "db_info": {
            "corpus-process": [
                {
                    "host": "http://elastic01:5004/v1.0/api/batch"
                },
                {
                    "host": "http://elastic02:5004/v1.0/api/batch"
                }
            ],
            "db_name": "khan_baseball"
        },
        "delay": "15~20",
        "document_id": "{_id}",
        "end": "2",
        "max_skip": 5000,
        "parsing_info": "khan.sports",
        "start": "1",
        "url_frame": [
            {
                "const_value": {
                    "section": "스포츠-야구"
                },
                "url": "http://news.khan.co.kr/kh_sports/baseball/index.html?&page={start}"
            }
        ]
    },
    "schedule": {
        "group": "aws",
        "mode": "daemon",
        "sleep": "20",
        "sleep_range": "03,04"
    },
    "state": {
        "host": "gollum01",
        "start": "1",
        "state": "running"
    }
}
