{
    "_id": "osen_sports",
    "docker": {
        "command": "python3 scheduler.py",
        "image": "crawler:1.0",
        "working_dir": "/usr/local/app"
    },
    "parameter": {
        "const_value": {
            "section": "스포츠-넥센"
