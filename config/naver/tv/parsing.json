      {
        "name": "div",
        "attribute": {
          "class": "list_body newsflash_body"
        }
      },
      {
        "name": "li"
      }
    ],
    "#list": "기사 목록에서 추출해야하는 하나의 기사 정보",
    "list": [
      {
        "comment": "제목",
        "key": "title",
        "type": "text",
        "tag": [
          {
            "name": "a"
          }
        ]
      },
      {
        "comment": "url",
        "key": "url",
        "type": "href",
        "tag": [
          {
            "name": "a"
          }
        ]
      },
      {
        "comment": "날짜",
        "key": "date",
        "type": "text",
        "type_convert": "date",
        "value_type": "single",
        "tag": [
          {
            "name": "span",
            "attribute": {
              "class": "date"
            }
          }
        ]
      },
      {
        "comment": "출처",
        "key": "source",
        "type": "text",
        "tag": [
          {
            "name": "span",
            "attribute": {
              "class": "writing"
            }
          }
        ]
      }
