          {
            "name": "div",
            "attribute": {
              "class": "article_info"
            }
          },
          {
            "name": "em"
          }
        ],
        "replace": [
          {
            "from": "오전(.+)$",
            "to": "\\g<1> AM"
          },
          {
            "from": "오후(.+)$",
            "to": "\\g<1> PM"
          }
