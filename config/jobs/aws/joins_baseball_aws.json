{
    "_id": "joins_baseball",
    "docker": {
        "command": "python3 scheduler.py",
        "image": "crawler:1.0",
        "working_dir": "/usr/local/app"
    },
    "parameter": {
        "db_info": {
            "corpus-process": [
                {
                    "host": "http://elastic01:5004/v1.0/api/batch"
                }
            ],
            "db_name": "joins_baseball"
        },
        "delay": "15~20",
        "document_id": "{_id}",
        "end_date": "2018-12-31",
        "max_skip": 5000,
        "parsing_info": "joins.sports",
        "start_date": "2018-01-01",
        "url_frame": [
            {
                "const_value": {
                    "section": "스포츠-야구"
                },
                "url": "http://news.joins.com/sports/baseball/list/1?filter=OnlyJoongang&date={date2}"
            }
        ]
    },
    "schedule": {
        "group": "aws",
        "mode": "daemon",
        "sleep": "20",
        "sleep_range": "03,04"
    },
    "state": {
        "state": "ready"
    }
}
