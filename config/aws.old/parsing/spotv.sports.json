{
   "_id": "spotv.sports",
   "article_page": [
      {
         "tag_name": "div",
         "attr": {
            "class": "arl_view_box"
         },
         "data": {
            "title": {
               "tag_name": "h1",
               "attr": {
                  "class": "arl_view_title"
               },
               "value": "text"
            },
            "author": {
               "tag_name": "span",
               "attr": {
                  "class": "arl_view_writer"
               },
               "value": "text"
            },
            "html_content": {
               "tag_name": "div",
               "attr": {
                  "itemprop": "articleBody"
               },
               "value": "html"
            }
         }
      }
   ],
   "comment": "SPOTV 야구 기사",
   "id": {
      "replace": [
         {
            "from": "^.+idxno=(.+?)[&].+$",
            "to": "\\g<1>"
         }
      ]
   },
   "page_list": {
      "index": {
         "tag_name": "a"
      },
      "next": {
         "attr": {
            "class": "p_next"
         },
         "tag_name": "a"
      },
      "panel": {
         "attr": {
            "class": "paginate"
         },
         "tag_name": "div"
      }
   },
   "parsing_type": "",
   "query_key_mapping": {
      "page": "start"
   },
   "subject_page": {
      "attr": {
         "class": "summary_list"
      },
      "next_tag": {
         "attr": {
            "class": "li title"
         },
         "data": {
            "date": {
               "attr": {
                  "class": "date"
               },
               "replace": [
                  {
                     "from": "^.+([1-2][09][0-9][0-9][.\\- :0-9]+).+$",
                     "to": "\\g<1>"
                  }
               ],
               "tag_name": "p",
               "value": "text"
            },
            "title": {
               "attr": {
                  "class": "title_a"
               },
               "tag_name": "a",
               "value": "text"
            },
            "url": {
               "tag_name": "a",
               "value": "href"
            }
         },
         "tag_name": "li"
      },
      "tag_name": "div"
   }
}