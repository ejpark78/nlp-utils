{
    "_id": "einfomax_finance",
    "docker": {
        "command": "python3 scheduler.py",
        "image": "crawler:1.0",
        "working_dir": "/usr/local/app"
    },
    "parameter": {
        "const_value": {
            "section": "정책/금융"
        },
        "db_info": {
            "corpus-process": [
                {
                    "host": "http://elastic01:5004/v1.0/api/batch"
                }
            ],
            "db_name": "einfomax_finance"
        },
        "delay": "20~30",
        "document_id": "{_id}",
        "end": "2",
        "max_skip": 5000,
        "parsing_info": "einfomax.finance",
        "start": "1",
        "url_frame": [
            {
                "const_value": {
                    "section": "정책/금융"
                },
                "url": "http://news.einfomax.co.kr/news/articleList.html?page={start}&sc_section_code=S1N15&sc_order_by=R"
            }
        ]
    },
    "schedule": {
        "group": "aws",
        "mode": "daemon",
        "sleep": "20",
        "sleep_range": "03,04"
    },
    "state": {}
}
