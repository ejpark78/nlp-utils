{
  "_id": "joins_live",
  "trace_list": {
    "parser": "html5lib",
    "#trace": "전체 html 에서 추출해야하는 전체 tag 목록",
    "trace": [
      {
        "name": "div",
        "attribute": {
          "class": "news_list"
        }
      },
      {
        "name": "li"
      }
    ],
    "#list": "기사 목록에서 추출해야하는 하나의 기사 정보",
    "list": [
      {
        "comment": "제목",
        "key": "title",
        "type": "text",
        "tag": [
          {
            "name": "dt"
          },
          {
            "name": "a",
            "attribute": {
              "class": "title_cr"
            }
          }
        ]
      },
      {
        "comment": "url",
        "key": "url",
        "type": "href",
        "tag": [
          {
            "name": "dt"
          },
          {
            "name": "a",
            "attribute": {
              "class": "title_cr"
            }
          }
        ]
      },
      {
        "comment": "날짜",
        "key": "date",
        "type": "text",
        "type_convert": "date",
        "value_type": "single",
        "tag": [
          {
            "name": "dd"
          },
          {
            "name": "span",
            "attribute": {
              "class": "date"
            }
          }
        ]
      },
      {
        "comment": "출처",
        "key": "source",
        "type": "text",
        "tag": [
          {
            "name": "dd"
          },
          {
            "name": "em",
            "attribute": {
              "class": "provide"
            }
          }
        ]
      }
    ],
    "#article": "기사 본문에서 추출해야하는 tag 정보",
    "article": [
      {
        "comment": "제목",
        "key": "title",
        "type": "text",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "article_tit"
            }
          },
          {
            "name": "h2"
          }
        ]
      },
      {
        "comment": "날짜",
        "key": "date",
        "type": "text",
        "type_convert": "date",
        "value_type": "single",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "write_info"
            }
          },
          {
            "name": "span",
            "attribute": {
              "class": "write"
            }
          }
        ],
        "replace": [
          {
            "from": "^.*입력",
            "to": ""
          }
        ]
      },
      {
        "comment": "날짜",
        "key": "edit_date",
        "type": "text",
        "type_convert": "date",
        "value_type": "single",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "write_info"
            }
          },
          {
            "name": "span",
            "attribute": {
              "class": "modify"
            }
          }
        ],
        "replace": [
          {
            "from": "^.*수정",
            "to": ""
          }
        ]
      },
      {
        "comment": "출처",
        "key": "source",
        "type": "text",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "write_info"
            }
          },
          {
            "name": "div",
            "attribute": {
              "class": "provider"
            }
          }
        ]
      },
      {
        "comment": "지면",
        "key": "paper_url",
        "type": "href",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "byline"
            }
          },
          {
            "name": "a"
          }
        ]
      },
      {
        "comment": "html 본문",
        "key": "html_content",
        "type": "html",
        "value_type": "merge",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "itemprop": "articleBody"
            }
          }
        ],
        "remove": [
          {
            "name": "iframe"
          },
          {
            "name": "script"
          },
          {
            "name": "div",
            "attribute": {
              "id": "top_ad"
            }
          },
          {
            "name": "div",
            "attribute": {
              "class": "article_msn_ad"
            }
          }
        ]
      },
      {
        "comment": "텍스트 본문",
        "key": "content",
        "type": "text",
        "value_type": "merge",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "itemprop": "articleBody"
            }
          }
        ]
      },
      {
        "comment": "이미지",
        "key": "photo_list",
        "type": "html",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "image"
            }
          },
          {
            "name": "img"
          }
        ]
      },
      {
        "comment": "이미지 캡션",
        "key": "photo_caption",
        "type": "text",
        "tag": [
          {
            "name": "p",
            "attribute": {
              "class": "caption"
            }
          }
        ]
      }
    ]
  }
}
