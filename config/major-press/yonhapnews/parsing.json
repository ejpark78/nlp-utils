{
  "_id": "yonhapnews",
  "trace_list": {
    "parser": "html5lib",
    "#trace": "전체 html 에서 추출해야하는 전체 tag 목록",
    "trace": [
      {
        "name": "li",
        "attribute": {
          "class": "section02"
        }
      },
      {
        "name": "div",
        "attribute": {
          "class": "con"
        }
      }
    ],
    "#list": "기사 목록에서 추출해야하는 하나의 기사 정보",
    "list": [
      {
        "comment": "제목",
        "key": "title",
        "type": "text",
        "tag": [
          {
            "name": "strong",
            "attribute": {
              "class": "news-tl"
            }
          },
          {
            "name": "a"
          }
        ]
      },
      {
        "comment": "url",
        "key": "url",
        "type": "href",
        "tag": [
          {
            "name": "strong",
            "attribute": {
              "class": "news-tl"
            }
          },
          {
            "name": "a"
          }
        ]
      }
    ],
    "#article": "기사 본문에서 추출해야하는 tag 정보",
    "article": [
      {
        "comment": "제목",
        "key": "title",
        "type": "text",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "id": "articleWrap"
            }
          },
          {
            "name": "h1",
            "attribute": {
              "class": "tit-article"
            }
          }
        ]
      },
      {
        "comment": "날짜",
        "key": "date",
        "type": "text",
        "type_convert": "date",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "id": "articleWrap"
            }
          },
          {
            "name": "span",
            "attribute": {
              "class": "tt"
            }
          }
        ],
        "replace": [
          {
            "from": "송고시간[ ]+[|][ ]+",
            "to": ""
          }
        ]
      },
      {
        "comment": "html 본문",
        "key": "html_content",
        "type": "html",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "id": "articleWrap"
            }
          },
          {
            "name": "div",
            "attribute": {
              "class": "article"
            }
          }
        ]
      },
      {
        "comment": "이미지",
        "key": "image_list.image",
        "type": "html",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "article-img"
            }
          },
          {
            "name": "img"
          }
        ]
      },
      {
        "comment": "이미지 캡션",
        "key": "image_list.caption",
        "type": "text",
        "delete": true,
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "img-info"
            }
          }
        ]
      },
      {
        "comment": "텍스트 본문",
        "key": "content",
        "type": "text",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "id": "articleWrap"
            }
          },
          {
            "name": "div",
            "attribute": {
              "class": "article"
            }
          }
        ]
      }
    ]
  }
}
