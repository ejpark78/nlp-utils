{
  "_id": "monoytoday",
  "trace_list": {
    "parser": "html5lib",
    "#trace": "전체 html 에서 추출해야하는 전체 tag 목록",
    "trace": [
      {
        "name": "li",
        "attribute": {
          "class": "bundle"
        }
      },
      {
        "name": "strong",
        "attribute": {
          "class": "subject"
        }
      }
    ],
    "#list": "기사 목록에서 추출해야하는 하나의 기사 정보",
    "list": [
      {
        "comment": "제목",
        "key": "title",
        "type": "text",
        "tag": [
          {
            "name": "a"
          }
        ]
      },
      {
        "comment": "url",
        "key": "url",
        "type": "href",
        "tag": [
          {
            "name": "a"
          }
        ]
      }
    ],
    "#article": "기사 본문에서 추출해야하는 tag 정보",
    "article": [
      {
        "comment": "제목",
        "key": "title",
        "type": "text",
        "value_type": "single",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "view_top"
            }
          },
          {
            "name": "h1"
          }
        ]
      },
      {
        "comment": "날짜",
        "key": "date",
        "type": "text",
        "type_convert": "date",
        "value_type": "single",
        "tag": [
          {
            "name": "ul",
            "attribute": {
              "class": "info2"
            }
          },
          {
            "name": "li",
            "attribute": {
              "class": "date"
            }
          }
        ],
        "replace": [
          {
            "from": "^.+([1-2][089][0-9][0-9][.\\- :0-9]+).+$",
            "to": "\\g<1>"
          }
        ]
      },
      {
        "comment": "글쓴이",
        "key": "author",
        "type": "text",
        "tag": [
          {
            "name": "a",
            "attribute": {
              "title": "기자의 다른기사보기"
            }
          }
        ]
      },
      {
        "comment": "html 본문",
        "key": "html_content",
        "type": "html",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "id": "textBody"
            }
          }
        ]
      },
      {
        "comment": "이미지",
        "key": "image_list.image",
        "type": "html",
        "tag": [
          {
            "name": "td",
            "attribute": {
              "class": "img"
            }
          },
          {
            "name": "img"
          }
        ]
      },
      {
        "comment": "이미지 갭션",
        "key": "image_list.caption",
        "type": "alt",
        "delete": true,
        "tag": [
          {
            "name": "td",
            "attribute": {
              "class": "img"
            }
          },
          {
            "name": "img"
          }
        ]
      },
      {
        "comment": "텍스트 본문",
        "key": "content",
        "type": "text",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "id": "textBody"
            }
          }
        ]
      }
    ]
  }
}
