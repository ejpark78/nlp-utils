{
  "_id": "joins",
  "trace_list": {
    "parser": "html5lib",
    "#trace": "전체 html 에서 추출해야하는 전체 tag 목록",
    "trace": [
      {
        "name": "div",
        "attribute": {
          "class": "list_basic"
        }
      },
      {
        "name": "li"
      }
    ],
    "#list": "기사 목록에서 추출해야하는 하나의 기사 정보",
    "list": [
      {
        "comment": "제목",
        "key": "title",
        "type": "text",
        "tag": [
          {
            "name": "strong",
            "attribute": {
              "class": "headline mg"
            }
          },
          {
            "name": "a"
          }
        ]
      },
      {
        "comment": "url",
        "key": "url",
        "type": "href",
        "tag": [
          {
            "name": "strong",
            "attribute": {
              "class": "headline mg"
            }
          },
          {
            "name": "a"
          }
        ]
      },
      {
        "comment": "날짜",
        "key": "date",
        "type": "text",
        "type_convert": "date",
        "value_type": "single",
        "tag": [
          {
            "name": "span",
            "attribute": {
              "class": "byline"
            }
          }
        ],
        "replace": [
          {
            "from": "^.+[|]",
            "to": ""
          }
        ]
      }
    ],
    "#article": "기사 본문에서 추출해야하는 tag 정보",
    "article": [
      {
        "comment": "제목",
        "key": "title",
        "type": "text",
        "tag": [
          {
            "name": "h1",
            "attribute": {
              "id": "article_title"
            }
          }
        ]
      },
      {
        "comment": "날짜",
        "key": "date",
        "type": "text",
        "type_convert": "date",
        "value_type": "single",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "byline"
            }
          }
        ],
        "replace": [
          {
            "from": "^.*입력",
            "to": ""
          },
          {
            "from": "^.*수정",
            "to": ""
          },
          {
            "from": "[|].+$",
            "to": ""
          }
        ]
      },
      {
        "comment": "출처",
        "key": "source",
        "type": "text",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "byline"
            }
          }
        ],
        "replace": [
          {
            "from": "입력.+$",
            "to": ""
          },
          {
            "from": "^.+수정",
            "to": ""
          },
          {
            "from": "[|].+$",
            "to": ""
          }
        ]
      },
      {
        "comment": "지면",
        "key": "paper_url",
        "type": "href",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "byline"
            }
          },
          {
            "name": "a"
          }
        ]
      },
      {
        "comment": "지면",
        "key": "paper_page",
        "type": "text",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "byline"
            }
          },
          {
            "name": "a"
          },
          {
            "name": "strong"
          }
        ]
      },
      {
        "comment": "글쓴이",
        "key": "author",
        "type": "text",
        "tag": [
          {
            "name": "span",
            "attribute": {
              "class": "profile"
            }
          },
          {
            "name": "strong",
            "attribute": {
              "class": "mg"
            }
          }
        ]
      },
      {
        "comment": "html 본문",
        "key": "html_content",
        "type": "html",
        "value_type": "merge",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "itemprop": "articleBody"
            }
          }
        ],
        "remove": [
          {
            "name": "iframe"
          },
          {
            "name": "script"
          },
          {
            "name": "div",
            "attribute": {
              "id": "criteo_network"
            }
          }
        ]
      },
      {
        "comment": "이미지",
        "key": "photo_list",
        "type": "html",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "image"
            }
          },
          {
            "name": "img"
          }
        ]
      },
      {
        "comment": "이미지 캡션",
        "key": "photo_caption",
        "type": "text",
        "delete": true,
        "tag": [
          {
            "name": "p",
            "attribute": {
              "class": "caption"
            }
          }
        ]
      },
      {
        "comment": "텍스트 본문",
        "key": "content",
        "type": "text",
        "value_type": "merge",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "itemprop": "articleBody"
            }
          }
        ]
      }
    ]
  }
}
