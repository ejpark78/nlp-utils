{
  "_id": "chosun",
  "trace_list": {
    "parser": "html5lib",
    "#trace": "전체 html 에서 추출해야하는 전체 tag 목록",
    "trace": [
      {
        "name": "dl",
        "attribute": {
          "class": "list_item"
        }
      }
    ],
    "#list": "기사 목록에서 추출해야하는 하나의 기사 정보",
    "list": [
      {
        "comment": "제목",
        "key": "title",
        "type": "text",
        "tag": [
          {
            "name": "dt"
          },
          {
            "name": "a"
          }
        ]
      },
      {
        "comment": "url",
        "key": "url",
        "type": "href",
        "tag": [
          {
            "name": "dt"
          },
          {
            "name": "a"
          }
        ]
      },
      {
        "comment": "날짜",
        "key": "date",
        "type": "text",
        "type_convert": "date",
        "value_type": "single",
        "tag": [
          {
            "name": "dd",
            "attribute": {
              "class": "date_author"
            }
          },
          {
            "name": "span",
            "attribute": {
              "class": "date"
            }
          }
        ],
        "replace": [
          {
            "from": "[(].+[)]",
            "to": ""
          }
        ]
      }
    ],
    "#article": "기사 본문에서 추출해야하는 tag 정보",
    "article": [
      {
        "comment": "제목",
        "key": "title",
        "type": "text",
        "tag": [
          {
            "name": "h1",
            "attribute": {
              "id": "news_title_text_id"
            }
          }
        ]
      },
      {
        "comment": "날짜",
        "key": "date",
        "type": "text",
        "type_convert": "date",
        "value_type": "single",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "news_date"
            }
          }
        ],
        "replace": [
          {
            "from": "입력",
            "to": ""
          },
          {
            "from": "^.+수정",
            "to": ""
          }
        ]
      },
      {
        "comment": "글쓴이",
        "key": "author",
        "type": "text",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "news_title_author"
            }
          },
          {
            "name": "li",
            "attribute": {
              "id": "j1"
            }
          }
        ]
      },
      {
        "comment": "글쓴이 소속",
        "key": "author_source",
        "type": "text",
        "tag": [
          {
            "name": "span",
            "attribute": {
              "class": "author_source"
            }
          }
        ]
      },
      {
        "comment": "html 본문",
        "key": "html_content",
        "type": "html",
        "value_type": "merge",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "par"
            }
          }
        ],
        "remove": [
          {
            "name": "iframe"
          },
          {
            "name": "script"
          }
        ]
      },
      {
        "comment": "이미지",
        "key": "photo_list",
        "type": "html",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "news_imgbox"
            }
          },
          {
            "name": "img"
          }
        ]
      },
      {
        "comment": "이미지 캡션",
        "key": "photo_caption",
        "type": "text",
        "delete": true,
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "news_imgbox"
            }
          }
        ]
      },
      {
        "comment": "텍스트 본문",
        "key": "content",
        "type": "text",
        "value_type": "merge",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "par"
            }
          }
        ]
      },
      {
        "comment": "다국어 기사",
        "key": "language_list",
        "type": "href",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "news_lang"
            }
          },
          {
            "name": "a"
          }
        ]
      }
    ]
  }
}
