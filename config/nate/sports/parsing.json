{
  "trace_list": {
    "parser": "lxml",
    "#trace": "전체 html 에서 추출해야하는 전체 tag 목록",
    "trace": [
      {
        "name": "div",
        "attribute": {
          "class": "postSubject"
        }
      },
      {
        "name": "li"
      }
    ],
    "#list": "기사 목록에서 추출해야하는 하나의 기사 정보",
    "list": [
      {
        "comment": "제목",
        "key": "title",
        "type": "text",
        "tag": [
          {
            "name": "a"
          }
        ]
      },
      {
        "comment": "url",
        "key": "url",
        "type": "href",
        "tag": [
          {
            "name": "a"
          }
        ]
      },
      {
        "comment": "날짜",
        "key": "date",
        "type": "text",
        "type_convert": "date",
        "value_type": "single",
        "tag": [
          {
            "name": "span",
            "attribute": {
              "class": "origin"
            }
          },
          {
            "name": "em"
          }
        ]
      }
    ],
    "#article": "기사 본문에서 추출해야하는 tag 정보",
    "article": [
      {
        "comment": "제목",
        "key": "title",
        "type": "text",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "id": "cntArea"
            }
          },
          {
            "name": "h3",
            "attribute": {
              "class": "viewTite"
            }
          }
        ]
      },
      {
        "comment": "날짜",
        "key": "date",
        "type": "text",
        "type_convert": "date",
        "value_type": "single",
        "tag": [
          {
            "name": "dl",
            "attribute": {
              "class": "articleInfo"
            }
          },
          {
            "name": "dd"
          },
          {
            "name": "em"
          }
        ]
      },
      {
        "comment": "출처",
        "key": "source",
        "type": "alt",
        "tag": [
          {
            "name": "dl",
            "attribute": {
              "class": "articleInfo"
            }
          },
          {
            "name": "img"
          }
        ]
      },
      {
        "comment": "html 본문",
        "key": "html_content",
        "type": "html",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "id": "articleContetns"
            }
          }
        ],
        "remove": [
          {
            "name": "caption"
          },
          {
            "name": "script"
          },
          {
            "name": "sclink"
          },
          {
            "name": "iframe"
          },
          {
            "name": "p",
            "attribute": {
              "style": "margin:0;padding:0;"
            }
          },
          {
            "name": "p",
            "attribute": {
              "style": "margin:0 0 20px 0;padding:0;"
            }
          }
        ]
      },
      {
        "comment": "이미지",
        "key": "image_list.image",
        "type": "html",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "id": "articleContetns"
            }
          },
          {
            "name": "img",
            "attribute": {
              "alt": "기사 이미지"
            }
          }
        ]
      },
      {
        "comment": "이미지 캡션",
        "key": "image_list.caption",
        "type": "text",
        "delete": true,
        "tag": [
          {
            "name": "div",
            "attribute": {
              "id": "articleContetns"
            }
          },
          {
            "name": "td",
            "attribute": {
              "class": "caption"
            }
          }
        ]
      },
      {
        "comment": "텍스트 본문",
        "key": "content",
        "type": "text",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "id": "articleContetns"
            }
          }
        ],
        "remove": [
          {
            "name": "caption"
          },
          {
            "name": "script"
          },
          {
            "name": "sclink"
          },
          {
            "name": "iframe"
          },
          {
            "name": "p",
            "attribute": {
              "style": "margin:0;padding:0;"
            }
          },
          {
            "name": "p",
            "attribute": {
              "style": "margin:0 0 20px 0;padding:0;"
            }
          }
        ]
      }
    ]
  }
}
