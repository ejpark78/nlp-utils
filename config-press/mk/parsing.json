{
  "_id": "mk",
  "trace_list": {
    "parser": "html5lib",
    "#trace": "전체 html 에서 추출해야하는 전체 tag 목록",
    "trace": [
      {
        "name": "div",
        "attribute": {
          "class": "list_area"
        }
      },
      {
        "name": "dl"
      }
    ],
    "#list": "기사 목록에서 추출해야하는 하나의 기사 정보",
    "list": [
      {
        "comment": "제목",
        "key": "title",
        "type": "text",
        "tag": [
          {
            "name": "dt",
            "attribute": {
              "class": "tit"
            }
          },
          {
            "name": "a"
          }
        ]
      },
      {
        "comment": "url",
        "key": "url",
        "type": "href",
        "tag": [
          {
            "name": "dt",
            "attribute": {
              "class": "tit"
            }
          },
          {
            "name": "a"
          }
        ]
      },
      {
        "comment": "날짜",
        "key": "date",
        "type": "text",
        "type_convert": "date",
        "value_type": "single",
        "tag": [
          {
            "name": "span",
            "attribute": {
              "class": "date"
            }
          }
        ]
      }
    ],
    "#article": "기사 본문에서 추출해야하는 tag 정보",
    "article": [
      {
        "comment": "제목",
        "key": "title",
        "type": "text",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "id": "view_tit"
            }
          },
          {
            "name": "span",
            "attribute": {
              "class": "head_tit"
            }
          }
        ]
      },
      {
        "comment": "제목",
        "key": "title",
        "type": "text",
        "tag": [
          {
            "name": "h1",
            "attribute": {
              "class": "top_title"
            }
          }
        ]
      },
      {
        "comment": "부제목",
        "key": "sub_title",
        "type": "text",
        "tag": [
          {
            "name": "h2",
            "attribute": {
              "class": "sub_title1"
            }
          }
        ]
      },
      {
        "comment": "날짜",
        "key": "date",
        "type": "text",
        "type_convert": "date",
        "value_type": "single",
        "tag": [
          {
            "name": "li",
            "attribute": {
              "class": "lasttime1"
            }
          }
        ],
        "replace": [
          {
            "from": "^.+(20[0-9][0-9][.\\- :0-9]+).+$",
            "to": "\\g<1>"
          }
        ]
      },
      {
        "comment": "html 본문",
        "key": "html_content",
        "type": "html",
        "value_type": "merge",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "id": "article_body"
            }
          }
        ]
      },
      {
        "comment": "이미지",
        "key": "image_list.image",
        "type": "src",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "class": "img_center"
            }
          },
          {
            "name": "img"
          }
        ]
      },
      {
        "comment": "이미지 캡션",
        "key": "image_list.caption",
        "type": "text",
        "delete": true,
        "tag": [
          {
            "name": "figure"
          },
          {
            "name": "figcaption"
          }
        ]
      },
      {
        "comment": "텍스트 본문",
        "key": "content",
        "type": "text",
        "value_type": "merge",
        "tag": [
          {
            "name": "div",
            "attribute": {
              "id": "article_body"
            }
          }
        ]
      }
    ]
  }
}
